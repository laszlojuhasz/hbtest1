<CODE>
#include <QtGui/QIcon>


/*
 * QIcon ()
 * QIcon ( const QPixmap & pixmap )
 * QIcon ( const QIcon & other )
 * QIcon ( const QString & fileName )
 * QIcon ( QIconEngine * engine )
 * QIcon ( QIconEngineV2 * engine )
 * ~QIcon ()
 */
HB_FUNC( QT_QICON )
{
   if( hb_pcount() == 1 )
   {
      if( HB_ISCHAR( 1 ) )
      {
         void * pText01 = NULL;
         __HB_RETPTRGC__( new QIcon( hb_parstr_utf8( 1, &pText01, NULL ) ) );
         hb_strfree( pText01 );
      }
      else if( hbqt_isObjectType( 1, HBQT_TYPE_QIcon ) )
      {   
         __HB_RETPTRGC__( new QIcon( *hbqt_par_QIcon( 1 ) ) );
      }   
      else if( hbqt_isObjectType( 1, HBQT_TYPE_QPixmap ) ) 
      {   
         __HB_RETPTRGC__( new QIcon( *hbqt_par_QPixmap( 1 ) ) );
      }   
      else
      {   
         hb_errRT_BASE( EG_ARG, 9999, NULL, HB_ERR_FUNCNAME, HB_ERR_ARGS_BASEPARAMS );   
      }
   }      
   else
   {
      __HB_RETPTRGC__( new QIcon() );
   }   
}
</CODE>

