 */
HB_FUNC( QT_QSIZEF )
{
   if( hb_pcount() == 2 && HB_ISNUM( 1 ) && HB_ISNUM( 2 ) )
   {
      __HB_RETPTRGC__( new QSizeF( hb_parnd( 1 ), hb_parnd( 2 ) ) );
   }
   else
   {
      __HB_RETPTRGC__( new QSizeF() );
   }
}
