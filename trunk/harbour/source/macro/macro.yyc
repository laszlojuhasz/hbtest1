yy_reduce_print (YYSTYPE *yyvsp, int yyrule, HB_MACRO_PTR pMacro)
#else
static void
yy_reduce_print (yyvsp, yyrule, pMacro)
    YYSTYPE *yyvsp;
    int yyrule;
