      CMDPREF := $(COMSPEC) /C
   endif
endif

ifneq ($(LIBNAME),)
   DIR_RULE := @$(CMDPREF)echo "The '$(LIBNAME)' library ignored."
else
   DIR_RULE :=
endif

all : first


first clean install::
	$(DIR_RULE)

endif
